@model TechBlog.Web.ViewModels.Articles.ArticlesViewModel

@foreach (var article in Model.Articles)
{
    <div class="col-md-12">
        <h2>
            <a href="@Url.Action("GetArticle", "Article", new { url = article.Url })">
                @article.Title
            </a>
        </h2>
        <p class="col-md-12">
            @article.Description
        </p>
    </div>
}
